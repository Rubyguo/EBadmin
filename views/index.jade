extends layout

block content
  #wrapper
    // 左侧导航开始
    nav.navbar-default.navbar-static-side(role='navigation', ng-app='myApp', ng-controller='userCtrl')
      .nav-close
        i.fa.fa-times-circle
      .sidebar-collapse
        ul#side-menu.nav
          li.nav-header
            .dropdown.profile-element
              span
                img.img-circle(alt='image', src='img/profile_small.jpg')
              a.dropdown-toggle(data-toggle='dropdown', href='#')
                span.clear
                  span.block.m-t-xs
                    strong.font-bold Beaut-zihan
                  span.text-muted.text-xs.block
                    | #{userrole}: #{username}
                    b.caret
              ul.dropdown-menu.animated.fadeInRight.m-t-xs
                li
                  a.J_menuItem(href='#') 修改头像
                li
                  a.J_menuItem(href='#') 个人资料
                li
                  a.J_menuItem(href='#') 联系我们
                li
                  a.J_menuItem(href='#') 信箱
                li.divider
                li
                  a(href='/login') 安全退出
            .logo-element
              | H+
          li#product
            a(href='#')
              i.fa.fa-home
              span.nav-label 主页
              span.fa.arrow
            ul.nav.nav-second-level
              li
                a.J_menuItem(href='/authorize') 待审批用户
              li
                a.J_menuItem(href='/') 主页示例二
              li
                a.J_menuItem(href='index_v3.html') 主页示例三
              li
                a.J_menuItem(href='index_v4.html') 主页示例四
              li
                a(href='index_v5.html', target='_blank') 主页示例五
          li#useradmin
            a.J_menuItem(href='/mainpage')
              i.fa.fa-columns
              span.nav-label 布局
          li#charts
            a(href='#')
              i.fa.fa.fa-bar-chart-o
              span.nav-label 统计图表
              span.fa.arrow
            ul.nav.nav-second-level
              li
                a.J_menuItem(href='/graph') 图表
              li
                a.J_menuItem(href='graph_flot.html') Flot
              li
                a.J_menuItem(href='graph_morris.html') Morris.js
              li
                a.J_menuItem(href='graph_rickshaw.html') Rickshaw
              li
                a.J_menuItem(href='graph_peity.html') Peity
              li
                a.J_menuItem(href='graph_sparkline.html') Sparkline
              li
                a.J_menuItem(href='graph_metrics.html') 图表组合
          li#email
            a(href='mailbox.html')
              i.fa.fa-envelope
              span.nav-label 信箱
              span.label.label-warning.pull-right 16
            ul.nav.nav-second-level
              li
                a.J_menuItem(href='mailbox.html') 收件箱
              li
                a.J_menuItem(href='mail_detail.html') 查看邮件
              li
                a.J_menuItem(href='mail_compose.html') 写信
          li#form
            a(href='#')
              i.fa.fa-edit
              span.nav-label 表单
              span.fa.arrow
            ul.nav.nav-second-level
              li
                a.J_menuItem(href='form_basic.html') 基本表单
              li
                a.J_menuItem(href='form_validate.html') 表单验证
              li
                a.J_menuItem(href='form_advanced.html') 高级插件
              li
                a.J_menuItem(href='form_wizard.html') 表单向导
              li
                a(href='#')
                  | 文件上传
                  span.fa.arrow
                ul.nav.nav-third-level
                  li
                    a.J_menuItem(href='form_webuploader.html') 百度WebUploader
                  li
                    a.J_menuItem(href='form_file_upload.html') DropzoneJS
                  li
                    a.J_menuItem(href='form_avatar.html') 头像裁剪上传
              li
                a(href='#')
                  | 编辑器
                  span.fa.arrow
                ul.nav.nav-third-level
                  li
                    a.J_menuItem(href='form_editors.html') 富文本编辑器
                  li
                    a.J_menuItem(href='form_simditor.html') simditor
                  li
                    a.J_menuItem(href='form_markdown.html') MarkDown编辑器
                  li
                    a.J_menuItem(href='code_editor.html') 代码编辑器
              li
                a.J_menuItem(href='suggest.html') 搜索自动补全
              li
                a.J_menuItem(href='layerdate.html') 日期选择器layerDate
    // 左侧导航结束
    // 右侧部分开始
    #page-wrapper.gray-bg.dashbard-1
      .row.border-bottom
        nav.navbar.navbar-static-top(role='navigation', style='margin-bottom: 0')
          .navbar-header
            a.navbar-minimalize.minimalize-styl-2.btn.btn-primary(href='#')
              i.fa.fa-bars
            form.navbar-form-custom(role='search', method='post', action='http://www.zi-han.net/theme/hplus/search_results.html')
              .form-group
                input#top-search.form-control(type='text', placeholder='请输入您需要查找的内容 …', name='top-search')

      .row.content-tabs
        button.roll-nav.roll-left.J_tabLeft
          i.fa.fa-backward
        nav.page-tabs.J_menuTabs
          .page-tabs-content
            a.active.J_menuTab(href='javascript:;', data-id='mainpage') 首页
        button.roll-nav.roll-right.J_tabRight
          i.fa.fa-forward
        .btn-group.roll-nav.roll-right
          button.dropdown.J_tabClose(data-toggle='dropdown')
            | 关闭操作
            span.caret
          ul.dropdown-menu.dropdown-menu-right(role='menu')
            li.J_tabShowActive
              a 定位当前选项卡
            li.divider
            li.J_tabCloseAll
              a 关闭全部选项卡
            li.J_tabCloseOther
              a 关闭其他选项卡
        a.roll-nav.roll-right.J_tabExit(href='/login')
          i.fa.fa.fa-sign-out
          |  退出
      #content-main.row.J_mainContent
        iframe.J_iframe(name='iframe0', width='100%', height='100%', src='/mainpage', frameborder='0', data-id='mainpage', seamless='')
      .footer
        .pull-right
          | © 2016
          //a(href='http://www.zi-han.net/', target='_blank') zihan's blog
    // 右侧部分结束
